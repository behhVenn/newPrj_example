        "@types/qs": "6.9.3",
        "@types/semver": "7.3.6",
        "@types/uuid": "8.3.0",
        "@types/web": "0.0.47",
        "@types/xml2js": "^0.4.9",
        "@typescript-eslint/eslint-plugin": "5.4.0",
        "@typescript-eslint/parser": "5.4.0",
        "@wdio/allure-reporter": "^7.20.7",
        "@wdio/cli": "^6.12.1",
        "@wdio/cucumber-framework": "^7.20.8",
        "@wdio/local-runner": "^7.20.9",
        "@wdio/spec-reporter": "^7.20.8",
        "backfill": "^4.0.0",
        "chromedriver": "^104.0.0",
