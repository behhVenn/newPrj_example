{
    "name": "@coxa-dri/automated-tests",
    "version": "0.1.0",
    "description": "dri test automation",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "zx": "node node_modules/zx/zx.mjs",
        "ts-node": "ts-node -r tsconfig-paths/register --",
        "generate:executor": "npm run ts-node ./src/scripts/executor.ts",
        "generate:environment": "npm run ts-node ./src/scripts/environment.ts",
        "get-secrets": "npm run ts-node ./src/services/awsServices/credentialService/credentialService.ts",
        "eslint": "npm run zx scripts/eslint.mjs",
        "lint": "npm run zx scripts/lint.mjs",
        "lint-fix": "npm run lint -- --fix",
        "build": "npm run zx scripts/build.mjs",
        "pretest": "npm run build",
        "test": "mocha --config ./_dist/config/unit.config.js --parallel",
        "test:ui": "npm run build && wdio ./_dist/config/config.js",
        "test:ui-local": "wdio ./_dist/config/config.js",
        "test:ui:saucelabs": "npm run build && cross-env SAUCELABS=true SAUCELABS_OS=macos wdio ./_dist/config/saucelabs.config.js",
        "test:api": "npm run build && mocha --config ./_dist/config/api.config.js --parallel",
        "test:api:single-thread": "npm run build && mocha --config ./_dist/config/api.config.js",
        "test:visual-regression": "rimraf ./src/visual-regression/tmp && cross-env CAPABILITIES=chrome_windows wdio ./_dist/config/config.js",
        "allure-report": "npm run generate:executor && npm run generate:environment && allure generate ./reports/allure-results -o ./reports/allure-reports --clean",
        "allure-report-open": "npm run allure-report && allure open ./reports/allure-reports",
        "send-allure-report": "npm run ts-node ./src/utils/slack/allure.ts",
        "gherkin-lint": "npm run zx scripts/gherkinlint.mjs",
        "test:pw-chromium": "node ./_dist/config/playwright/start-parallel-tests.js",
        "test:pw-webkit": "cross-env PW_BROWSER=webkit node ./_dist/config/playwright/start-parallel-tests.js",
        "test:pw-mobile": "cross-env PW_BROWSER=webkit MOBILE=true node ./_dist/config/playwright/start-parallel-tests.js",
        "test:pw-mobile-iPhone11Pro": "cross-env PW_BROWSER=webkit MOBILE=true PW_DEVICE=iPhone11Pro node ./_dist/config/playwright/start-parallel-tests.js",
        "cms:get-content": "npm run ts-node ./src/services/cmsService/get-CMS-content.ts",
        "cms:set-content": "npm run ts-node ./src/services/cmsService/set-CMS-content.ts",
        "tenant-service:erase-aqa-tenants": "npm run ts-node ./src/services/onboarding/erase-aqa-tenants.ts",
        "doctor": "cross-env SPEC_PATH=true npm run zx scripts/doctor.mjs",
        "prepare": "husky install",
        "coxa": "npm run ts-node ./src/cli/index.ts --",
        "wdio": "wdio run wdio.conf.js"
    },
    "engines": {
        "node": ">=14",
        "npm": ">=7"
    },
    "engineStrict": true,
    "keywords": [],
    "author": "",
    "devDependencies": {
        "@npmcli/ci-detect": "1.4.0",
        "@types/aws4": "^1.11.0",
        "@types/chai": "4.2.22",
        "@types/copyfiles": "2.4.1",
        "@types/cucumber": "6.0.1",
        "@types/eslint": "^8.2.0",
        "@types/faker": "5.5.9",
        "@types/jsonfile": "6.0.1",
        "@types/jwt-decode": "2.2.1",
        "@types/lodash": "4.14.161",
